<%- model_class = DominioValor -%>
<div class="row">
    <div class="col-lg-12">
        <div class="ibox float-e-margins">
            <div class="ibox-content">
                <p id="notice"><%= notice %></p>
                <form class="form-horizontal">
                    <div class="form-group">
                        <div class="col-md-2 control-label">
                             <%= model_class.human_attribute_name(:codigo) %>:
                        </div>
                        <div class="col-md-10">
                            <label class="form-control">
                                <%= @dominio_valor.codigo %>
                            </label>
                        </div>
                    </div>
                    <div class="hr-line-dashed"></div>

                                    <div class="form-group">
                        <div class="col-md-2 control-label">
                             <%= model_class.human_attribute_name(:descricao) %>:
                        </div>
                        <div class="col-md-10">
                            <label class="form-control">
                                <%= @dominio_valor.descricao %>
                            </label>
                        </div>
                    </div>
                    <div class="hr-line-dashed"></div>

                                    <div class="form-group">
                        <div class="col-md-2 control-label">
                             <%= model_class.human_attribute_name(:dominio) %>:
                        </div>
                        <div class="col-md-10">
                            <label class="form-control">
                                <%= @dominio_valor.dominio_id %>
                            </label>
                        </div>
                    </div>
                    <div class="hr-line-dashed"></div>

                                <div class="form-group">
                    <div class="col-sm-4">
                        <%= link_to edit_dominio_valor_path(@dominio_valor), class: 'btn btn-primary' do %>
                        Editar
                        <% end %>
                        <%= link_to 'Voltar', dominio_valores_path, :class => 'btn btn-white' %>
                    </div>
                </div>

                </form>

            </div>
        </div>
    </div>
</div>
