                                                    <th><%= model_class.human_attribute_name(:valorTotalNfe) %></th>
                                                    <th><%= model_class.human_attribute_name(:dtEmissaoNfe) %></th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr>
                                                    <td style="vertical-align:middle;" align="center">
                                                    <%= form_tag negar_nfe_nota_fiscais_path, method: :post do %>
                                                        <%= hidden_field_tag :id, @nota_fiscal.id %>
                                                        <%= hidden_field_tag :tpEvento,  NotaFiscal.tipo_eventos[:"Desconhecimento da Operação"] %>
                                                        <%= hidden_field_tag :justificativa, "" %>
                                                        <%= submit_tag "Negar", class: "btn btn-xs", style:"width: 95px;background-color:#BD1D1D;color:white;", id: "negar" %>
                                                    <% end %>
                                                    </td>
                                                    <td style="vertical-align:middle;"><%= select_tag :codSituacaoManifestacaoDestinatario, options_for_select(NotaFiscal.codSituacaoManifestacaoDestinatarios.keys[2,2]), { class: "form-control" } %></td>
                                                    <td align="center" style="width:200px;"><textarea disabled id="texto_justificativa" title="Tamanho mínimo 15 caracteres!" placeholder="Obrigátorio para operação não realizada..." ></textarea></td>
                                                    <td style="vertical-align:middle;"><%= @nota_fiscal.nrChaveNfe %></td>
                                                    <td style="vertical-align:middle;"><%= @nota_fiscal.nomeEmitente %></td>
                                                    <td style="vertical-align:middle;"><%= number_with_precision(@nota_fiscal.valorTotalNfe, precision: 2) %></td>
                                                    <td style="vertical-align:middle;"><%= @nota_fiscal.dtEmissaoNfe.strftime("%d/%m/%Y") %></td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>                              
                                <% end %>
                            <% end %>
                        </div>
                    </div>
                </div>
                <%= link_to 'Voltar', nota_fiscais_path, :class => 'btn btn-white' %>
            </div>
        </div>
    </div>
</div>


<% content_for :javascript do %>
    <script type="text/javascript">
        $(function(){
            $('#codSituacaoManifestacaoDestinatario').change(function(){
                var tipo_manifesto = document.getElementById("codSituacaoManifestacaoDestinatario").value;
                if (tipo_manifesto == "Desconhecida") {  
                    document.getElementById('tpEvento').value = "210220"; // Desconhecimento da Operação com o enum em nota_fiscal
                    document.getElementById("texto_justificativa").disabled = true;
                    document.getElementById("texto_justificativa").value = "";
                    document.getElementById("negar").disabled = false;
                } else {
                    document.getElementById('tpEvento').value = "210240"; // Operação não Realizada de acordo com o enum nota_fiscal
                    document.getElementById("texto_justificativa").disabled = false;
                    document.getElementById("negar").disabled = true;
                };

            });

            $('#texto_justificativa').keyup(function(){
                if (document.getElementById("texto_justificativa").value.length >= 15 ){
                    document.getElementById('justificativa').value = document.getElementById("texto_justificativa").value;
                    document.getElementById("negar").disabled = false;
                } else {
                    document.getElementById("negar").disabled = true;
                }
            });
        });
    </script>    
<% end %>