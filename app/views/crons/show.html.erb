<%- model_class = Cron -%>
<div class="row">
    <div class="col-lg-12">
        <div class="ibox float-e-margins">
            <div class="ibox-content">
                <p id="notice"><%= notice %></p>
                <form class="form-horizontal">
                    <div class="form-group">
                        <div class="col-md-2 control-label">
                             <%= model_class.human_attribute_name(:data) %>:
                        </div>
                        <div class="col-md-10">
                            <label class="form-control">
                                <%= @cron.data %>
                            </label>
                        </div>
                    </div>
                    <div class="hr-line-dashed"></div>

                                    <div class="form-group">
                        <div class="col-md-2 control-label">
                             <%= model_class.human_attribute_name(:cnpj) %>:
                        </div>
                        <div class="col-md-10">
                            <label class="form-control">
                                <%= @cron.cnpj %>
                            </label>
                        </div>
                    </div>
                    <div class="hr-line-dashed"></div>

                                    <div class="form-group">
                        <div class="col-md-2 control-label">
                             <%= model_class.human_attribute_name(:xml_retorno) %>:
                        </div>
                        <div class="col-md-10">
                            <textarea class="form-control">
                                <%= @cron.xml_retorno %>
                            </textarea>
                        </div>
                    </div>
                    <div class="hr-line-dashed"></div>

                                    <div class="form-group">
                        <div class="col-md-2 control-label">
                             <%= model_class.human_attribute_name(:mensagem) %>:
                        </div>
                        <div class="col-md-10">
                            <label class="form-control">
                                <%= @cron.mensagem %>
                            </label>
                        </div>
                    </div>
                    <div class="hr-line-dashed"></div>

                                <div class="form-group">
                    <div class="col-sm-4">
                        <%#= link_to edit_crom_path(@crom), class: 'btn btn-primary' do %>
                        <!-- Editar -->
                        <%# end %>
                        <%= link_to 'Voltar', crons_path, :class => 'btn btn-white' %>
                        <%#= link_to 'Novo', new_crom_path, :class => 'btn btn-warning' %>

                    </div>
                </div>

                </form>

            </div>
        </div>
    </div>
</div>
