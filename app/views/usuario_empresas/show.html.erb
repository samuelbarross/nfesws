<%- model_class = UsuarioEmpresa -%>
<div class="row">
    <div class="col-lg-12">
        <div class="ibox float-e-margins">
            <div class="ibox-content">
                <p id="notice"><%= notice %></p>
                <form class="form-horizontal">
                    <div class="form-group">
                        <div class="col-md-2 control-label">
                             <%= model_class.human_attribute_name(:user) %>:
                        </div>
                        <div class="col-md-10">
                            <label class="form-control">
                                <%= @usuario_empresa.user %>
                            </label>
                        </div>
                    </div>
                    <div class="hr-line-dashed"></div>

                                    <div class="form-group">
                        <div class="col-md-2 control-label">
                             <%= model_class.human_attribute_name(:empresa) %>:
                        </div>
                        <div class="col-md-10">
                            <label class="form-control">
                                <%= @usuario_empresa.empresa %>
                            </label>
                        </div>
                    </div>
                    <div class="hr-line-dashed"></div>

                                <div class="form-group">
                    <div class="col-sm-4">
                        <%= link_to edit_usuario_empresa_path(@usuario_empresa), class: 'btn btn-primary' do %>
                        Editar
                        <% end %>
                        <%= link_to 'Voltar', usuario_empresas_path, :class => 'btn btn-white' %>
                        <%= link_to 'Novo', new_usuario_empresa_path, :class => 'btn btn-warning' %>

                    </div>
                </div>

                </form>

            </div>
        </div>
    </div>
</div>
