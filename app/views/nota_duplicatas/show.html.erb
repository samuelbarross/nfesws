<%- model_class = NotaDuplicata -%>
<div class="row">
    <div class="col-lg-12">
        <div class="ibox float-e-margins">
            <div class="ibox-content">
                <p id="notice"><%= notice %></p>
                <form class="form-horizontal">
                    <div class="form-group">
                        <div class="col-md-2 control-label">
                             <%= model_class.human_attribute_name(:nrDuplicata) %>:
                        </div>
                        <div class="col-md-10">
                            <label class="form-control">
                                <%= @nota_duplicata.nrDuplicata %>
                            </label>
                        </div>
                    </div>
                    <div class="hr-line-dashed"></div>

                                    <div class="form-group">
                        <div class="col-md-2 control-label">
                             <%= model_class.human_attribute_name(:dtVencimento) %>:
                        </div>
                        <div class="col-md-10">
                            <label class="form-control">
                                <%= @nota_duplicata.dtVencimento %>
                            </label>
                        </div>
                    </div>
                    <div class="hr-line-dashed"></div>

                                    <div class="form-group">
                        <div class="col-md-2 control-label">
                             <%= model_class.human_attribute_name(:valorDuplicata) %>:
                        </div>
                        <div class="col-md-10">
                            <label class="form-control">
                                <%= @nota_duplicata.valorDuplicata %>
                            </label>
                        </div>
                    </div>
                    <div class="hr-line-dashed"></div>

                                    <div class="form-group">
                        <div class="col-md-2 control-label">
                             <%= model_class.human_attribute_name(:notaFiscal) %>:
                        </div>
                        <div class="col-md-10">
                            <label class="form-control">
                                <%= @nota_duplicata.notaFiscal %>
                            </label>
                        </div>
                    </div>
                    <div class="hr-line-dashed"></div>

                                <div class="form-group">
                    <div class="col-sm-4">
                        <%= link_to edit_nota_duplicata_path(@nota_duplicata), class: 'btn btn-primary' do %>
                        Editar
                        <% end %>
                        <%= link_to 'Voltar', nota_duplicatas_path, :class => 'btn btn-white' %>
                    </div>
                </div>

                </form>

            </div>
        </div>
    </div>
</div>
